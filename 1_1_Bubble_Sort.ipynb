      "source": [
        "def Bubble_Sort (A):\n",
        "  for i in range (1,len(A)):\n",
        "    for j in range (0,len(A)-1):\n",
        "      if A[j]>A[j+1]:\n",
        "        A[j],A[j+1] = A[j+1],A[j]\n",
        "  return A"
      ],
      "metadata": {
